{
  "name": "@TAK-NZ/base-infra",
  "version": "0.1.0",
  "description": "Base infrastructure for TAK-NZ projects using AWS CDK",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "unset CDK_DEFAULT_ACCOUNT && unset CDK_DEFAULT_REGION && jest",
    "cdk": "cdk",
    "clean": "rm -f bin/*.js bin/*.d.ts lib/*.js lib/*.d.ts test/*.js test/*.d.ts; rm -rf cdk.out/*"
  },
  "devDependencies": {
    "@aws-cdk/assertions": "^1.203.0",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1017.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.198.0",
    "constructs": "^10.0.0"
  }
}
